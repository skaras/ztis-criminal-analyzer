<script type="text/javascript">
function $(e){return document.getElementById(e)}
function box_icv(n){
  for(var i=1;i<4;i++){
    if ($('box_icon'+i)){
      if (i==n){
        $('box_icon'+i).style.display='block'
        $('nav_icon'+i).style.color='#a00000'
        $('nav_icon'+i).style.background='#ccc'
      }else{
        $('box_icon'+i).style.display='none'
        $('nav_icon'+i).style.color='inherit'
        $('nav_icon'+i).style.background='#eee'
      }
    }
  }
}
</script>

